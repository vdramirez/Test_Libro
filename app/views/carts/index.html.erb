<p id="notice"><%= notice %></p>

<h1>Carts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>




    <% @carts.each do |cart| %>
      <tr>
        <td><%= link_to 'Show', cart %></td>
        <td><%= link_to 'Edit', edit_cart_path(cart) %></td>
        <td><%= button_to 'Empty cart', cart, :method => :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<%= link_to 'New Cart', new_cart_path %>
